{
  "files.autoSave": "afterDelay",
  "emmet": {
    "includeLanguages": {
      "javascript": "javascriptreact",
      "aspnetcorerazor": "html",
      "razor": "html"
    },
    "triggerExpansionOnTab": true,
    "showSuggestionsAsSnippets": true,
    "useInlineCompletions": true
  },
  "liveServer": {
    "settings.donotShowInfoMsg": true,
    "settings.donotVerifyTags": true
  },
  "explorer": {
    "confirmDelete": false,
    "confirmDragAndDrop": false,
    "compactFolders": false
  },
  "git": {
    "enableSmartCommit": true,
    "autofetch": true,
    "confirmSync": false
  },
  "workbench": {
    "editor.untitled.hint": "hidden",
    "startupEditor": "none",
    "iconTheme": "material-icon-theme",
    "editorAssociations": {
      "*.pdf": "default"
    }
  },
  "editor": {
    "linkedEditing": true,
    "accessibilitySupport": "on",
    "defaultFormatter": "esbenp.prettier-vscode",
    "formatOnSave": true
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "security.workspace.trust.untrustedFiles": "open",
  "php.validate.executablePath": "D:/wamp64/bin/php/php7.4.26/php.exe",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.wordWrap": "on",
  "[javascript]": {
    "editor.formatOnSave": true
  },
  "files.associations": {
    "*.cshtml": "aspnetcorerazor"
  },
  "[razor]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "[aspnetcorerazor]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "window.zoomLevel": 1,
  "git.ignoreRebaseWarning": true,
  "html.format.wrapLineLength": 100,
  "editor.renderWhitespace": "none",
  "window.commandCenter": true,
  "editor.fontFamily": "JetBrains Mono",
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 1.7,
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "workbench.colorTheme": "GitHub Dark Default",
  "editor.codeLensFontFamily": "JetBrains Mono",
  "scm.inputFontFamily": "JetBrains Mono",
  "debug.console.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "editor.inlayHints.fontFamily": "JetBrains Mono",
  "notebook.outputFontFamily": "JetBrains Mono"
}
